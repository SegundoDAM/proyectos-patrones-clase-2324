import java.util.Random;

public class Generator implements Runnable{

	private int awesomeValue=0;
	
	@Override
	public void run() {
		do {
			try {
				Thread.sleep(1000);
				awesomeValue=new Random().nextInt(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}while(true);
	}

	public int getAwesomeValue() {
		return awesomeValue;
	}

	
}
