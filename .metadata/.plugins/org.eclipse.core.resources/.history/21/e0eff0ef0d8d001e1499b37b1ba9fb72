package profesor01;

import java.nio.channels.SocketChannel;

import profesor01.StatePattern.State;

public class Context {
	/**
	 * 
	 */
	private final StatePattern Context;

	/**
	 * @param statePattern
	 */
	Context(StatePattern statePattern) {
		Context = statePattern;
	}

	private State state;

	public void setState(State state) {
		this.state = state;
	}

	public State getState() {
		return state;
	}

	public SocketChannel request() {
		return state.processState();
	}
}