
public class SingletonPerrezoso {
	private int cosa;
	private static SingletonPerrezoso INSTANCIA=new SingletonPerrezoso();

	private SingletonPerrezoso() {
		super();
	}

	public int getCosa() {
		return cosa;
	}

	public void setCosa(int cosa) {
		this.cosa = cosa;
	}

	public static SingletonPerrezoso getINSTANCIA() {
		return INSTANCIA;
	}
	
	
}
