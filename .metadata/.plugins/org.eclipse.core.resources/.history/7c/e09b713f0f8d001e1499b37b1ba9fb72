package esquema00;

public class Context implements Strategy {
	public Strategy state;

	public Context(Strategy state) {
		super();
		this.state = state;
	}

	public void changeState(Strategy state) {
		this.state = state;
	}

	@Override
	public void doThis() {
		state.doThis();
	}

	@Override
	public void doThat() {
		state.doThat();
	}

}
