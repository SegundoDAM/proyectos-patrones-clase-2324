package profesor01;

import java.io.IOException;
import java.nio.channels.SocketChannel;

public class CloseSocketState implements State {
	/**
	 * 
	 */
	private final StatePattern CloseSocketState;
	SocketChannel socketChannel;

	public CloseSocketState(StatePattern statePattern, SocketChannel socketChannel) {
		CloseSocketState = statePattern;
		this.socketChannel = socketChannel;
	}

	public SocketChannel processState() {
		try {
			socketChannel.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
		return socketChannel;
	}
}